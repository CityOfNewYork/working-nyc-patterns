/ Next/Previous Button

button class='btn ${this.button.class}' tabindex=this.button.tabindex type='button'
  svg class='icon-wnyc-ui' aria-hidden='true' tabindex=this.button.tabindex
    use xlink:href='#feather-arrow-left'

  span = this.button.label

  svg class='icon-wnyc-ui' aria-hidden='true' tabindex=this.button.tabindex
    use xlink:href='#feather-arrow-right'
